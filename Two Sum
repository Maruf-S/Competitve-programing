class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        array_len = len(nums)
        for i in range(array_len):
            for j in range(array_len):
                if nums[i] + nums[j] ==target and i!=j:
                    return [i,j]
